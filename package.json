{
  "name": "kamalio",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "test": "npm run test",
    "build": "babel src -d dist",
    "start": "node --harmony dist/server",
    "watch": "babel -w src -d dist",
    "watch-d": "babel -w src -d dist & nodemon --harmony dist/server",
    "drop-db": "node ./dist/tools/drop-database.js",
    "sync-db": "node ./dist/tools/sync-database.js",
    "seed-db": "node ./dist/tools/seed-database.js"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.13.15"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
